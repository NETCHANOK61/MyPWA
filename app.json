{
  "expo": {
    "newArchEnabled": true,
    "entryPoint": "./src/main.js",
    "name": "MyPWA",
    "slug": "MyPWA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_pwa.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon_pwa.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_pwa.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
